		<script type="text/javascript">
			$(document).ready(function(){
					
			});

			function ajaxFileUpload(func_type) {
				var loading_icon = "#loading_" + func_type;
		        $(loading_icon)
		        .ajaxStart(function() {
		            $(this).show();
		        })
		        .ajaxComplete(function() {
		            $(this).hide();
		        });
		        var dest_url = '/updatedata_' + func_type + '/' + $("#updatedata_date_year").val() + "/" + $("#updatedata_date_month").val() + "/";
	        	var elementId = func_type + '_file_id';
	        	var myStatus = '#updatedata_files_item_status_' + func_type;
				$.ajaxFileUpload
		        (
		            {
		                url: dest_url, 
		                secureuri: false,
		                fileElementId: elementId,
		                dataType: 'json',
		                success: function (data, status) {
		                    if(typeof(data.error) != 'undefined') {
		                        if(data.error != '') {
		                            // alert("1-" + data.error);
		                        } else {
		                            // alert("2-" + data.msg);
		                        }
		                        $(myStatus).text('已上传').addClass("updatedata_tips");
		                    }
		                },
		                error: function (data, status, e) {
		                    // alert("3-" + e);
		                    $(myStatus).text('已上传').addClass("updatedata_tips");
		                }
		            }
		        )
		        return false;
		    } 
		</script>	
